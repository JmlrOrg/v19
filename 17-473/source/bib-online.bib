@article{Main1979,
author = "Main, P.",
title = "{A theoretical comparison of the {$\beta$},{$\gamma$}' and 2{\it F}${\sb {\it o}}${--}{\it F}${\sb {\it c}}$ syntheses}",
journal = "Acta Crystallographica Section A",
year = "1979",
volume = "35",
number = "5",
pages = "779--785",
month = "Sep",
doi = {10.1107/S0567739479001789},
url = {http://dx.doi.org/10.1107/S0567739479001789},
abstract = {A theoretical comparison of the {\it {$\beta$}} synthesis of Ramachandran & Raman [{\it Acta Cryst.} (1959), {\bf 12}, 957-964] with the weighted {\it {$\gamma$}}' and weighted 2{\it F${\sb o}$} - {\it F${\sb c}$} syntheses shows that, for partially known structures, the ratio of peak heights of the unknown atoms to those of the background is the same in all the syntheses. The {\it {$\beta$}} synthesis contains more background peaks than either the {\it {$\gamma$}}' or 2{\it F${\sb o}$} - {\it F${\sb c}$} syntheses. The 2{\it F${\sb o}$} - {\it F${\sb c}$} synthesis suppresses the peaks of wrongly positioned atoms most effectively and the {\it {$\gamma$}}' is the least effective. The syntheses which approach most closely the true electron density are the weighted 2{\it F${\sb o}$} - {\it F${\sb c}$} synthesis for non-centrosymmetric structures and the weighted {\it {$\gamma$}}' for centrosymmetric ones.},
}


