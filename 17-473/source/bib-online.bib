    @article{MM_tutorial2004,
     jstor_articletype = {research-article},
     title = {A Tutorial on MM Algorithms},
     author = {Hunter, David R. and Lange, Kenneth},
     journal = {The American Statistician},
     jstor_issuetitle = {},
     volume = {58},
     number = {1},
     jstor_formatteddate = {Feb., 2004},
     pages = {pp. 30-37},
     url = {http://www.jstor.org/stable/27643496},
     ISSN = {00031305},
     abstract = {Most problems in frequentist statistics involve optimization of a function such as a likelihood or a sum of squares. EM algorithms are among the most effective algorithms for maximum likelihood estimation because they consistently drive the likelihood uphill by maximizing a simple surrogate function for the log-likelihood. Iterative optimization of a surrogate function as examplified by an EM algorithm does not necessarily require missing data. Indeed, every EM algorithm is a special case of the more general class of MM optimization algorithms, which typically exploit convexity rather than missing data in majorizing or minorizing an objective function. In our opinion, MM algorithms deserve to be part of the standard toolkit of professional statisticians. This article explains the principle behind MM algorithms, suggests some methods for constructing them, and discusses some of their attractive features. We include numerous examples throught the article to illustrate the concepts described. In addition to surveying previous work on MM algorithms, this article introduces some new material on constrained optimization and standard error estimation.},
     language = {English},
     year = {2004},
     publisher = {American Statistical Association},
     copyright = {Copyright ?? 2004 American Statistical Association},
    }
    
    @article{Lerman2014,
abstract = {This work presents a fast and non-convex algorithm for robust subspace recovery. The data sets considered include inliers drawn around a low-dimensional subspace of a higher dimensional ambient space, and a possibly large portion of outliers that do not lie nearby this subspace. The proposed algorithm, which we refer to as Fast Median Subspace (FMS), is designed to robustly determine the underlying subspace of such data sets, while having lower computational complexity than existing methods. We prove convergence of the FMS iterates to a stationary point. Further, under a special model of data, FMS converges to a point which is near to the global minimum with overwhelming probability. Under this model, we show that the iteration complexity is globally bounded and locally {\$}r{\$}-linear. The latter theorem holds for any fixed fraction of outliers (less than 1) and any fixed positive distance between the limit point and the global minimum. Numerical experiments on synthetic and real data demonstrate its competitive speed and accuracy.},
archivePrefix = {arXiv},
arxivId = {1406.6145},
author = {Lerman, Gilad and Maunu, Tyler},
eprint = {1406.6145},
month = {jun},
title = {{Fast, Robust and Non-convex Subspace Recovery}},
url = {http://arxiv.org/abs/1406.6145},
year = {2014}
}

    
